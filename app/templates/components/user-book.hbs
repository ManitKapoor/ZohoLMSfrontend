<div class="container-fluid">
  <div class="panel panel-success">
    <div class="panel panel-heading"><span class="glyphicon glyphicon-book"></span> My Books</div>
    <div class="panel panel-body">
       <table class="table table-condensed">
       <thead>
         <tr>
         <th>Title</th>
         <th>Author</th>
         <th>Edition</th>
         <th>Issued Date</th>
         <th>Return Date</th>
         </tr>
       </thead>
       <tbody>
        {{#each dat as |book|}}
          <tr>
          <td>{{book.title}}</td>
          <td>{{book.author}}</td>
          <td>{{book.edition}}</td>
          <td>{{book.issueddate}}</td>
          <td>{{book.returndate}}</td>
           </tr>
         {{/each}}
       </tbody>
        </table>
    </div>
  </div>
</div>